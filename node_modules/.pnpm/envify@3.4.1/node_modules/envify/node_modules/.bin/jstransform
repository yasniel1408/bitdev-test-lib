#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/bin/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/jstransform@11.0.3/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/bin/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/jstransform@11.0.3/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jstransform@11.0.3/node_modules/jstransform/bin/jstransform" "$@"
else
  exec node  "$basedir/../../../../../jstransform@11.0.3/node_modules/jstransform/bin/jstransform" "$@"
fi
