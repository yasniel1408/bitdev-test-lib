#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules/@teambit/bit/bin/node_modules:/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules/@teambit/bit/node_modules:/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules/@teambit/node_modules:/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules:/Users/yasniel/.bvm/versions/1.5.29/node_modules:/Users/yasniel/.bvm/versions/node_modules:/Users/yasniel/.bvm/node_modules:/Users/yasniel/node_modules:/Users/node_modules:/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules/@teambit/bit/bin/node_modules:/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules/@teambit/bit/node_modules:/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules/@teambit/node_modules:/Users/yasniel/.bvm/versions/1.5.29/bit-1.5.29/node_modules:/Users/yasniel/.bvm/versions/1.5.29/node_modules:/Users/yasniel/.bvm/versions/node_modules:/Users/yasniel/.bvm/node_modules:/Users/yasniel/node_modules:/Users/node_modules:/node_modules:/Users/yasniel/Documents/PROYECTOS/cencosud/bitdev-test-lib/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@teambit/bit/bin/bit" "$@"
else
  exec node  "$basedir/../@teambit/bit/bin/bit" "$@"
fi
